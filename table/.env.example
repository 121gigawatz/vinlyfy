# Flask environment
FLASK_EVV=production
DEBUG_MODE=false

# Security
SECRET_KEY=your-secret-key-changeme-for-production

# Server Configuration
TABLE_PORT=5000

# File Upload Settings
MAX_UPLOAD_MB=25

# Processed File Storage
# Files expire after this many hours
FILE_TTL_HOURS=1

# CORS Settings
# Use '*' for development, specific origins for production
# Example: http://localhost:3000,https://yourdomain.com
CORS_ORIGINS=*